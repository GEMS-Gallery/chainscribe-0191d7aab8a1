type Schedule = vec Game;
type Result = 
 variant {
   err: text;
   ok: Schedule;
 };
type Game = 
 record {
   away: text;
   date: text;
   home: text;
   time: text;
 };
service : {
  getScheduleForTeam: (text) -> (Result) query;
  getTeams: () -> (vec text) query;
  initializeSchedules: () -> ();
}
